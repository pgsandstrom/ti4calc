{
  "name": "ti4-calc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "format": "prettier --write \"{,!(.next|dist|node_modules)/**/}!(package-lock).{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix .",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && cross-env CI=true npm run test && npm run typecheck",
    "start": "next start -p 8200",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/lodash": "4.14.186",
    "@types/pg": "8.6.5",
    "@types/react": "18.0.24",
    "@types/styled-components": "5.1.26",
    "cross-env": "7.0.3",
    "husky": "8.0.1",
    "lodash": "4.17.21",
    "next": "12.1.5",
    "pg": "8.7.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.6",
    "typescript": "4.8.4",
    "worker-loader": "3.0.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "12.1.5",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest": "27.5.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1"
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix",
      "prettier --write"
    ]
  }
}
